{
  "name": "vuejs_boilerplate",
  "version": "0.1.0",
  "private": true,
  "author": "Arun Redhu",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'"
  },
  "dependencies": {
    "@progress/kendo-datasource-vue-wrapper": "^2019.3.1023",
    "@progress/kendo-dropdowntree-vue-wrapper": "^2019.3.1023",
    "@progress/kendo-grid-vue-wrapper": "^2019.3.1023",
    "@progress/kendo-inputs-vue-wrapper": "^2019.3.1023",
    "@progress/kendo-layout-vue-wrapper": "^2019.3.1023",
    "@progress/kendo-popups-vue-wrapper": "^2019.3.1023",
    "@progress/kendo-theme-bootstrap": "^4.4.0",
    "@progress/kendo-ui": "^2019.3.1106",
    "axios": "^0.18.1",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.4",
    "core-js": "^2.6.5",
    "vee-validate": "^3.0.11",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.5",
    "vue-i18n": "^8.15.0",
    "vue-router": "^3.0.3",
    "vue-shortkey": "^3.1.7",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "homepage": "https://github.com/arunredhu/vuejs_boilerplate"
}
